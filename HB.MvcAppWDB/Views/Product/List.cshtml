@{
    ViewData["Title"] = "List";
}

@model List<Product>


<table class="table">

    <thead>
        <tr>
            <th>ıd</th>
            <th>name</th>
            <th>price</th>
        </tr>
    </thead>
    <tbody>
        @{

            foreach (var item in Model)
            {
                <tr>
                <td>@item.Id</td>
                <td>@item.Name</td>
                <td>@item.Price</td>
                </tr>
            }
        }
    </tbody>

</table>
